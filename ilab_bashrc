# If not running interactively, don't do anything
[ -z "$PS1" ] && return 

if [ -f /etc/bash_completion.d/git ]
then
	source /etc/bash_completion.d/git
fi

#all the cool programs
export PATH='/usr/unsupported/bin:/filer/tmp2/contrib/bin:/usr/lib/qt-3.3/bin:/usr/kerberos/bin:/usr/local/bin:/bin:/usr/bin'

#all the cool libs
export LD_LIBRARY_PATH="/usr/unsupported/lib:/filer/tmp2/contrib/lib"

#all the cool directories
alias filer="cd /filer/tmp2; ls"
alias contrib="cd /filer/tmp2/contrib; ls"
alias mine='cd /filer/tmp2/jmatth; ls'

#my files
umask 077
